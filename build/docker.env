ENV=dev

GRPC_SERVER_HOST=127.0.0.1
GRPC_SERVER_PORT=8080
GRPC_SERVER_IDLE_CONNECTION_TIMEOUT_IN_MINUTES=5
GRPC_PROMETHEUS_HTTP_PORT=8080

HTTP_SERVER_HOST=127.0.0.1
HTTP_SERVER_PORT=8080
HTTP_SERVER_READ_TIMEOUT_IN_SEC=5
HTTP_SERVER_WRITE_TIMEOUT_IN_SEC=5

LOG_LEVEL=info
LOG_SINK=stdout,file

LOG_FILE_NAME=app.log
LOG_FILE_DIR=./
LOG_FILE_MAX_SIZE_IN_MB=50
LOG_FILE_MAX_BACKUPS=1
LOG_FILE_MAX_AGE=1
LOG_FILE_WITH_LOCAL_TIME_STAMP=true

